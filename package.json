{
  "name": "essentials",
  "version": "1.2.0",
  "description": "The Khan Academy utility library.",
  "main": "essentials.js",
  "scripts": {
    "lint": "npx eslint src/**",
    "test": "npx karma start karma.config.js",
    "build:cdn": "npx rollup --config && node scripts/terser.js",
    "build:ka": "node scripts/babel.js",
    "build": "npx rollup --config && concurrently \"npm:build:ka\" \"node scripts/terser.js\"",
    "doc:html": "npx jsdoc -c jsdoc.json",
    "doc:md": "node scripts/wiki.js && npx jsdoc2md build/essentials-x.js > internal/Essentials-Documentation.md",
    "doc:ka": "concurrently \"node scripts/showdown.js\" \"node scripts/inline-remote-assets.js\"",
    "doc:format": "node scripts/prettier.js",
    "doc": "concurrently \"npm:doc:html\" \"npm:doc:md\" && npm run doc:ka && npm run doc:format",
    "dev:build": "npx rollup --config dev/rollup.config.js",
    "dev": "npm run dev:build && call dev/start.bat"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhavjitChauhan/Essentials.git"
  },
  "author": "Bhavjit Chauhan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bhavjitChauhan/Essentials/issues"
  },
  "homepage": "https://github.com/bhavjitChauhan/Essentials#readme",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-arrow-functions": "^7.13.0",
    "@babel/plugin-transform-block-scoping": "^7.14.4",
    "@babel/plugin-transform-destructuring": "^7.13.17",
    "@babel/plugin-transform-exponentiation-operator": "^7.12.13",
    "@babel/plugin-transform-parameters": "^7.14.2",
    "@babel/plugin-transform-spread": "^7.13.0",
    "@babel/plugin-transform-template-literals": "^7.13.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "chai": "^4.3.4",
    "chalk": "^4.1.1",
    "concurrently": "^6.2.0",
    "docdash": "^1.2.0",
    "eslint": "^7.27.0",
    "inline-remote-assets": "^0.7.0",
    "jsdoc": "^3.6.7",
    "jsdoc-to-markdown": "^7.0.1",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-json-fixtures-preprocessor": "^0.0.6",
    "karma-mocha": "^2.0.1",
    "karma-requirejs": "^1.1.0",
    "mocha": "^8.4.0",
    "prettier": "^2.3.0",
    "rollup": "^2.50.2",
    "showdown": "^1.9.1",
    "terser": "^5.7.0"
  }
}
