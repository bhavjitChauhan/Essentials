{
  "name": "essentials",
  "version": "2.0.0beta",
  "description": "The Khan Academy utility library.",
  "main": "essentials.js",
  "scripts": {
    "lint": "npx eslint src/**",
    "test": "npx karma start karma.config.js",
    "build:cdn": "npx rollup --config && node lib/build-cdn-compress.js",
    "build:ka": "node lib/build-ka.js",
    "build": "npx rollup --config && concurrently \"npm:build:ka\" \"node lib/build-cdn-compress.js\"",
    "doc:html": "npx jsdoc -c jsdoc.json",
    "doc:format": "node lib/doc-format.js",
    "doc": "npm run doc:html && npm run doc:format",
    "dev:build": "npx rollup --config dev/rollup.config.js",
    "dev:watch": "npx watch \"npm run dev:build\" src",
    "dev:serve": "call dev/start.bat",
    "dev": "concurrently \"npm:dev:watch\" \"npm:dev:serve\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhavjitChauhan/Essentials.git"
  },
  "author": "Bhavjit Chauhan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bhavjitChauhan/Essentials/issues"
  },
  "homepage": "https://github.com/bhavjitChauhan/Essentials#readme",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-transform-arrow-functions": "^7.14.5",
    "@babel/plugin-transform-block-scoping": "^7.14.5",
    "@babel/plugin-transform-destructuring": "^7.14.7",
    "@babel/plugin-transform-exponentiation-operator": "^7.14.5",
    "@babel/plugin-transform-for-of": "^7.14.5",
    "@babel/plugin-transform-parameters": "^7.14.5",
    "@babel/plugin-transform-spread": "^7.14.6",
    "@babel/plugin-transform-template-literals": "^7.14.5",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "chai": "^4.3.4",
    "chalk": "^4.1.1",
    "concurrently": "^6.2.0",
    "docdash": "^1.2.0",
    "eslint": "^7.31.0",
    "inline-remote-assets": "^0.7.0",
    "jsdoc": "^3.6.7",
    "jsdoc-to-markdown": "^7.0.1",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-json-fixtures-preprocessor": "^0.0.6",
    "karma-mocha": "^2.0.1",
    "karma-requirejs": "^1.1.0",
    "mocha": "^9.0.2",
    "prettier": "^2.3.2",
    "rollup": "^2.53.3",
    "terser": "^5.7.1",
    "watch": "^1.0.2"
  }
}
